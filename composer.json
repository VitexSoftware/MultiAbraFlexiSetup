{
    "name": "vitexsoftware/multiflexibee-setup",
    "description": "Tool used to setup FlexiBee multiinstance",
    "version": "0.3",
    "type": "project",
    "require": {
        "vitexsoftware/ease-twbootstrap4-widgets-flexibee": "*",
        "vitexsoftware/ease-fluentpdo": "*",
        "vlucas/phpdotenv": "^3",
        "vitexsoftware/ease-twbootstrap4-widgets": "*",
        "robmorgan/phinx": "*",
        "datatables/datatables": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\MultiSetup\\": "src/FlexiPeeHP/MultiSetup/",
            "FlexiPeeHP\\MultiSetup\\Ui\\": "src/FlexiPeeHP/MultiSetup/Ui"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\FlexiPeeHP\\": "vendor/spoje.net/flexibee/testing/src/FlexiPeeHP/",
            "Test\\FlexiPeeHP\\MultiSetup\\": "tests/FlexiPeeHP/MultiSetup/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.2"
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('vendor/datatables/datatables/media/js/dataTables.bootstrap4.js', 'src/js/dataTables.bootstrap4.js');copy('vendor/datatables/datatables/media/js/jquery.dataTables.js', 'src/js/jquery.dataTables.js');copy('vendor/datatables/datatables/media/css/dataTables.bootstrap4.css','src/css/dataTables.bootstrap4.css');\""
        ],
        "post-update-cmd": [
            "php -r \"copy('vendor/datatables/datatables/media/js/dataTables.bootstrap4.js', 'src/js/dataTables.bootstrap4.js');copy('vendor/datatables/datatables/media/js/jquery.dataTables.js', 'src/js/jquery.dataTables.js');copy('vendor/datatables/datatables/media/css/dataTables.bootstrap4.css','src/css/dataTables.bootstrap4.css');\""
        ]
    }
    
}
